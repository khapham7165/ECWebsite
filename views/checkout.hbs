    <!-- Start All Title Box -->
    <div class="all-title-box">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <h2>Checkout</h2>
                    <ul class="breadcrumb">
                        <li class="breadcrumb-item"><a href="/">Shop</a></li>
                        <li class="breadcrumb-item active">Checkout</li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <!-- End All Title Box -->

    <!-- Start Cart  -->
    <div class="cart-box-main">
        <div class="container">
            {{#if isLoggedIn}}
            {{else}}
            <div class="row new-account-login">
                <div class="col-sm-6 col-lg-6 mb-3">
                    <div class="title-left" >
                        <h3>Account Login</h3>
                    </div>
                    <h5><a data-toggle="collapse" href="#formLogin" role="button" aria-expanded="false">Click here to Login</a></h5>
                    <form class="mt-3 collapse review-form-box" id="formLogin" action = "users/login" method = "post">
                        <div class="form-row">
                            <div class="form-group col-md-6">
                                <label for="email" class="mb-0" >Email Address</label>
                                <input type="email" class="form-control" id="email" name = "email" placeholder="Enter Email"> </div>
                            <div class="form-group col-md-6">
                                <label for="password" class="mb-0">Password</label>
                                <input type="password" class="form-control" id="password" name ="password" placeholder="Password"> </div>
                        </div>
                        <button type="submit" class="btn hvr-hover">Login</button>
                    </form>
                </div>
                <div class="col-sm-6 col-lg-6 mb-3">
                    <div class="title-left">
                        <h3>Create New Account</h3>
                    </div>
                    <h5><a data-toggle="collapse" href="#formRegister" role="button" aria-expanded="false">Click here to Register</a></h5>
                    <form class="mt-3 collapse review-form-box" id="formRegister" action = "users/register" method = "POST" >
                        <div class="form-row">
                            <div class="form-group col-md-6">
                                <label for="InputName" class="mb-0">Full name</label>
                                <input type="text" class="form-control" id="username" name = "username" placeholder="Full name"> </div>
                            <div class="form-group col-md-6">
                                <label for="InputEmail1" class="mb-0">Email Address</label>
                                <input type="email" class="form-control" id="email" name = "email" placeholder="Enter Email"> </div>
                            <div class="form-group col-md-6">
                                <label for="InputLastname" class="mb-0">Password</label>
                                <input type="password" class="form-control" id="password" name= "password" placeholder="Password"> </div>
                            
                            <div class="form-group col-md-6">
                                <label for="confirmPassword" class="mb-0">Confirm Password</label>
                                <input type="password" class="form-control" id="password2" name = "password2" placeholder="Confirm your password"> </div>
                        </div>
                        <button type="submit" class="btn hvr-hover">Register</button>
                    </form>
                </div>
            </div>
            {{/if}}
            <div class="row">
                <div class="col-sm-6 col-lg-6 mb-3">
                    <div class="checkout-address">
                        <div class="title-left">
                            <h3>Payment</h3>
                        </div>
                        <form class="needs-validation" novalidate>
                            <div class="title"> <span>Payment method</span> </div>
                            <div class="d-block my-3">
                                <div class="custom-control custom-radio">
                                    <input id="credit" name="paymentMethod" type="radio" class="custom-control-input" checked required>
                                    <label class="custom-control-label" for="credit">Paypal</label>
                                </div>
                            </div>
                            <div class="row">
                                
                            </div>
                            <div class="row">
                                
                            </div>
                            <hr class="mb-1"> </form>
                    </div>
                </div>
                {{#if isLoggedIn}}
                {{#with cart}}
                <div class="col-sm-6 col-lg-6 mb-3">
                    <div class="row">
                        <div class="col-md-12 col-lg-12">
                            <div class="odr-box">
                                <div class="title-left">
                                    <h3>Shopping cart</h3>
                                </div>
                                {{#if items.length}}
                                    {{#each items}}
                                    <div class="rounded p-2 bg-light">
                                        <div class="media mb-2 border-bottom">
                                            <div class="media-body"> <a href="/detail"> {{item.name}}</a>
                                                <div class="small text-muted">Price: {{item.price}} </div>
                                            </div>
                                        </div>
                                    </div>
                                    {{/each}}
                                {{/if}}
                            </div>
                        </div>
                        {{#if items.length}}
                            <div class="col-md-12 col-lg-12">
                                    <hr>
                                    <div class="d-flex gr-total">
                                        <h5>Grand Total</h5>
                                        <div class="ml-auto h5"> ₫ {{totalPrice}} </div>
                                    </div>
                                    <hr> </div>
                            </div>
                            <div class="col-12 d-flex shopping-box"> <a href="/users/pay" class="ml-auto btn hvr-hover">Place Order</a> </div>
                        {{else}}
                            <div class="d-flex gr-total">
                                        <h5>  There's no course in the cart</h5>
                            </div>
                        {{/if}}
                    </div>
                </div>
            </div>
            {{/with}}
            {{else}}
            {{#with cart}}
                <div class="col-sm-6 col-lg-6 mb-3">
                    <div class="row">
                        <div class="col-md-12 col-lg-12">
                            <div class="odr-box">
                                <div class="title-left">
                                    <h3>Shopping cart</h3>
                                </div>
                                {{#if items.length}}
                                    {{#each items}}
                                    <div class="rounded p-2 bg-light">
                                        <div class="media mb-2 border-bottom">
                                            <div class="media-body"> <a href="/detail"> {{item.name}}</a>
                                                <div class="small text-muted">Price: {{item.price}} </div>
                                            </div>
                                        </div>
                                    </div>
                                    {{/each}}
                                {{/if}}
                            </div>
                        </div>
                        {{#if items.length}}
                            <div class="col-md-12 col-lg-12">
                                    <hr>
                                    <div class="d-flex gr-total">
                                        <h5>Grand Total</h5>
                                        <div class="ml-auto h5"> ₫ {{totalPrice}} </div>
                                    </div>
                                    <hr> </div>
                            </div>
                            <div class="col-12 d-flex shopping-box"> <a href="/users/login" class="ml-auto btn hvr-hover">Please Log in to countinue!</a> </div>
                        {{else}}
                            <div class="d-flex gr-total">
                                        <h5>  There's no course in the cart</h5>
                            </div>
                        {{/if}}
                    </div>
                </div>
            </div>
            {{/with}}
            {{/if}}
        </div>
    </div>
    <!-- End Cart -->