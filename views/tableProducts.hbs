        <div class="breadcrumbs">
            <div class="breadcrumbs-inner">
                <div class="row m-0">
                    <div class="col-sm-4">
                        <div class="page-header float-left">
                            <div class="page-title">
                                <h1>Dashboard</h1>
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-8">
                        <div class="page-header float-right">
                            <div class="page-title">
                                <ol class="breadcrumb text-right">
                                    <li><a href="/admin">Dashboard</a></li>
                                    <li><a href="#">Thống kê</a></li>
                                    <li class="active">Thống kê khóa học</li>
                                </ol>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="content">
            <div class="animated fadeIn">
                <div class="row">

                    <div class="col-md-12">
                        <div class="card">
                            <div class="card-header">
                                <strong class="card-title">Danh sách khóa học</strong>
                            </div>
                            <div class="card-body">
                                <table id="bootstrap-data-table" class="table table-striped table-bordered">
                                    <thead>
                                        <tr>
                                            <th></th>
                                            <th>Id</th>
                                            <th>Name</th>
                                            <th>Category</th>
                                            <th>Price</th>
                                            {{!-- <th>Imagepath</th> --}}
                                            {{!-- <th>Videopath</th> --}}
                                            <th>Status</th>
                                            {{!-- <th>Summary</th> --}}
                                            {{!-- <th>Description</th> --}}
                                            <th>Sold</th>
                                            <th>Total Reviews</th>
                                            <th>Rating</th>    
                                        </tr>
                                    </thead>
                                    <tbody>
                                        {{#each products}}
                                            <tr>
                                                <th><a href="./tableProductDetail/{{id}}">Full details</a></th>
                                                <th>{{id}}</th>
                                                <th>{{name}}</th>
                                                <th>{{Category.name}}</th>
                                                <th>{{price}}</th>
                                                {{!-- <th>{{imagepath}}</th> --}}
                                                {{!-- <th>{{videopath}}</th> --}}
                                                <th>{{status}}</th>
                                                {{!-- <th>{{summary}}</th> --}}
                                                {{!-- <th>{{description}}</th> --}}
                                                <th>{{sold}}</th>
                                                <th>{{reviewCount}}</th>
                                                <th>{{overallReview}}</th>
                                            </tr>
                                        {{/each}}
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div><!-- .animated -->
        </div><!-- .content -->

        
